        <script>
            var DrawTableInquiry;

            $(document).ready(function () {
                getF21_SRNO();

                var dataColumns = [{
                    data: "_id",
                    sTitle: "Id",
                    visible: false
                },
                {
                    data: "tranNo",
                    sTitle: "Sr No",
                    visible: true,
                    className: "fs15--design column-50",
                },

                {
                    data: "rsid",
                    sTitle: "RSID",
                    visible: true,
                    className: "fs15--design column-50",
                },


                // {
                //     data: "dongleCat",
                //     sTitle: "Dongle Cat.",
                //     visible: true,
                //     className: "fs15--design column-50",
                // },

                // {
                //     data: "dongleSubCat",
                //     sTitle: "Sub Category",
                //     visible: true,
                //     className: "fs15--design column-100",
                // },

                {
                    data: "MSISDN",
                    sTitle: "MSISDN",
                    visible: true,
                    className: "fs15--design column-50",
                },

                {
                    data: "simNo",
                    sTitle: "SIM Number",
                    visible: true,
                    className: "fs15--design column-150",
                },

                {
                    data: "name",
                    sTitle: "Name",
                    visible: true,
                    className: "fs15--design column-200",
                },

                {
                    data: "location",
                    sTitle: "Location",
                    visible: true,
                    className: "fs15--design column-50",
                },

                {
                    data: "department",
                    sTitle: "Department",
                    visible: true,
                    className: "fs15--design column-100",
                },

                {
                    data: "action",
                    sTitle: "Action",
                    visible: true,
                    className: "fs15--design column-50",
                    render: function (data, type, row) {
                        //console.log(row);
                        var html =
                            ' <span class="btn btn-primary btn-block" data-id="' + row
                                ._id +
                            '" onclick="getUser(this) "><i class="fa fa-pencil"></i> Edit</span>';
                        return html;
                    }
                }

                ];

                DrawTableInquiry = DrawCustomDataTable("uspList", 350,
                    "/route/jioDongle/grid-data", dataColumns, [
                    [0, "asc"]
                ], "frm_grid_search", null, true, true);

                $("#frm_grid_search").validate({
                    errorPlacement: function (error, element) { },
                    submitHandler: function (form) {
                        DrawTableInquiry.ajax.reload();
                    }
                });

                $('.tab-a').click(function () {
                    $(".tab").removeClass('tab-active');
                    $(".tab[data-id='" + $(this).attr('data-id') + "']").addClass("tab-active");
                    $(".tab-a").removeClass('active-a');
                    $(this).parent().find(".tab-a").addClass('active-a');
                });

                $('.textUpper').on('keyup', function (e) {
                    $(this).val($(this).val().toUpperCase());
                });

                $("form#regForm").submit(function (e) {
                    // $('#newUserBox').modal('hide');
                    e.preventDefault();
                });

                $("#regForm").validate({
                    rules: {
                    },
                    messages: {
                    },
                    errorPlacement: function (error, element) {
                        offset = element.offset();
                        error.insertAfter(element)
                        error.addClass('message');
                        error.css('display', 'block!important');
                        error.css('color', 'red!important');

                    },
                    submitHandler: function (form) {
                        $('#newUserBox').modal('hide');
                        var $form = $(form);
                        var formData = $form.serializeObject();
                        var method = "post";
                        var url = "/route/jioDongle/create";
                        console.log(formData.id);
                        if (formData.id != 0) {
                            url =
                                '/route/jioDongle/updateData/' +
                                formData.id;
                            method = "put";
                        }
                        $.ajax({
                            url: url,
                            type: method,
                            contentType: "application/json; charset=utf-8",
                            data: JSON.stringify(formData),
                            //data: formData,
                            dataType: "json",
                            success: function (result) {

                                if (result.status) {
                                    notification({
                                        type: "success",
                                        message: result.msg

                                    });
                                    DrawTableInquiry.ajax.reload();

                                    //$("#regForm")[0].reset();
                                }

                            }
                        });
                    }
                });
            });

            $(document).on("click", "#AddData", function () {
                $('.uniqueID').val("0");
                $("#newUserBox").modal("show");
                DrawTableInquiry.ajax.reload();
                $("#regForm")[0].reset();
                $("#F21_SRNO").select2();
                $("#F21_SRNO")[0].reset();
            });

            function populate(frm, data) {
                $.each(data, function (key, value) {
                    $('[name=' + key + ']', frm).val(value);
                });
                $('.sector').select2();
                $("#F21_SRNO").select2();
            }

            function getUser(id) {
                var id = id.getAttribute('data-id');
                $('.uniqueID').val(id);
                $.ajax({
                    url: "/route/jioDongle/getdata",
                    type: "get",
                    data: {
                        id: id
                    },
                    success: function (result) {

                        var iddata = {
                            id: result.data._id
                        }
                        $("#F21_SRNO").select2();
                        populate('#regForm', result.data);
                        populate('#regForm', iddata);
                        $('#newUserBox').modal('show');

                    }
                })
            }

            function openNav() {
                document.getElementById("mySidenav").style.width = "250px";
            }

            function closeNav() {
                document.getElementById("mySidenav").style.width = "0";
            }


            function getF21_SRNO() {
                var tray_url = "/route/jioDongle/getF21_SRNO";
                $.getJSON(tray_url, function (data) {
                    var optionList3 = '<option value="">Select Name</option>';
                    for (var i = 0; i < data.length; i++) {
                        //console.log("optionList1", data[i]);
                        optionList3 += '<option value="' + data[i].F21_SRNO + '">' + data[i].F21_SRNO + ' - ' + data[i].F21_FNAME + ' ' + data[i].F21_SNAME + '</option>';
                    }

                    $('#F21_SRNO').html(optionList3);
                    $('#F21_SRNO').select2();
                });
            }

            function setEmployeeDetails() {
                var obj = {
                    "F21_SRNO": $('#F21_SRNO').val(),
                    //"F_HLPNAME": e,
                };
                // console.log("objobjobjobj", obj)
                $.ajax({
                    method: "POST",
                    url: "/route/jioDongle/setEmployee",
                    data: JSON.stringify(obj),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (result) {
                        //console.log("resultresultresult", result)
                        $('#employee').val(result.Data.F21_NAME);
                        $('#loc').val(result.Data.F21_BRANCH);
                        $('#depart').val(result.Data.F21_DEPT);

                    }
                });
            }

            function test1() {
                DrawTableInquiry.ajax.reload();
            }
        </script>



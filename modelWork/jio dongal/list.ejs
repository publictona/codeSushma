                $.getJSON(tray_url, function (data) {
                    var optionList3 = '<option value="">Branch</option>';
                    for (var i = 0; i < data.length; i++) {
                        //  console.log("optionList1",data)
                        optionList3 += '<option value="' + data[i] + '">' +

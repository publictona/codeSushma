        .banner::before {
            content: "";
            background-image: url("/assets/images/pdfAssets/brush1.png");
            position: absolute;
            top: -10px;
            z-index: 1;
            height: 100%;
            width: 100%;
            background-repeat: repeat-x;
        }

        .banner::after {
            content: "";
            background-image: url("/assets/images/pdfAssets/brush1.png");
            position: absolute;
            bottom: -3px;
            z-index: 1;
            height: 100%;
            width: 100%;
            background-repeat: repeat-x;
            transform: rotate(180deg);
        }

        .number {

            font-size: 16px;
            font-weight: 700;
        }

        .number-of-days {
            display: flex;
            align-items: baseline;

                    background-image: url("/assets/images/pdfAssets/close.jpg");
                    width: 14px;
                    height: 14px;
                    background-size: cover;
                    background-repeat: no-repeat;
                }

                &:not(:last-child) {
                    margin-bottom: 10px;
                }
            }
        }

        .s-note-list {
            padding-left: 20px;
            margin-top: 10px;
            margin-bottom: 30px;


            >li {
                &:not(:last-child) {
                    margin-bottom: 10px;
                }
            }
        }

        table {
            width: 100%;
            border-collapse: collapse;
        }

        th,
        td {
            text-align: left;
            padding: 8px;
            border: 1px solid #ccc;
        }

        .faq-question {
            font-weight: bold;
            margin-bottom: 10px;

        }

        .faq-answer {
            margin-bottom: 20px;
        }

        .review-card-list {

      <script>
        $(document).ready(function () {
          $('.datepicker').datepicker({
            format: 'dd/mm/yyyy',
            startDate: '01/01/1900',
            autoclose: true
          });





          $(window).load(function () {
            $(".select2").select2();
          });
          var language = { AccessToken: "<%= AccessToken %>" };
          fill_module_data("language", "language", "Select Language", "language", "language", "GET", language)
          $('.btn-submit').attr('disabled', 'disabled');
          var SubmitUrlId;
          var populate_data = {};
          var action_type;
          var NewObject;
          var flag = false;
          $("#erp_form .form-control").on("blur change keyup", function () {
            $(this).closest('form').data('changed', true);
            // if(flag == true)
            $('.btn-submit').removeAttr('disabled');
          });

          $(".form_add_entry").click(function () {
            document.getElementById("erp_form").reset();
            $(".select2").select2();
            action_type = "POST";
            $(".form-title").html("Add Wedding Checklist");
            $(".button-submit-form").html("Add Checklist");
            $(".form_add_entry").hide();
          });
          $(document).on('click', '.grid-edit-row', function () {
            $(".overlay").show();
            $(".form-title").html("Edit Wedding Checklist");
            $(".button-submit-form").html("Update Checklist");
            $('.remove_member').each(function () {
              $(this).parent().remove();
            });
            document.getElementById("erp_form").reset();
            $(".select2").select2();
            var id = $(this).attr("data-id");
            var url = "/route/weddingPlanCheckList/" + id;
            $.get(url, {}, function (data) {
              populate_data = data;
              $.when($('#erp_form').populate(data)).done(function (x) {
                SubmitUrlId = "/" + populate_data._id;
                action_type = "PUT";
                NewObject = data;
                flag = true;
                if (data.checkList) {
                  var len = data.checkList.length;
                  for (var i = 1; i <= len; i++) {
                    if (i != len) {
                      $("#cloneButton").click();
                    }

                    var selector = i - 1;
                    // $('#erp_form').populate(data.checkList[selector])
                    // console.log(data.checkList[selector])
                    $('#membar_Narrow_down_dates_' + i).val(data.checkList[selector]["Narrow_down_dates"]);
                    $('#membar_gather_guest_list_' + i).val(data.checkList[selector]["gather_guest_list"]);
                    $('#membar_settle_a_budget_' + i).val(data.checkList[selector]["settle_a_budget"]);
                    $('#membar_create_wedding_website_' + i).val(data.checkList[selector]["create_wedding_website"]);
                    $('#membar_reserve_venue_' + i).val(data.checkList[selector]["reserve_venue"]);
                    $('#membar_book_vendors_' + i).val(data.checkList[selector]["book_vendors"]);
                    $('#membar_final_number_of_events_' + i).val(data.checkList[selector]["final_number_of_events"]);
                    $('#membar_book_entertainment_' + i).val(data.checkList[selector]["book_entertainment"]);
                    $('#membar_wedding_trousou_' + i).val(data.checkList[selector]["wedding_trousou"]);
                    $('#membar_wedding_outfits_' + i).val(data.checkList[selector]["wedding_outfits"]);
                    $('#membar_invitation_selection_' + i).val(data.checkList[selector]["invitation_selection"]);
                    $('#membar_book_car_tranportation_' + i).val(data.checkList[selector]["book_car_tranportation"]);
                    $('#membar_make_up_artist_' + i).val(data.checkList[selector]["make_up_artist"]);
                    $('#membar_acessory_shopping_' + i).val(data.checkList[selector]["acessory_shopping"]);
                    $('#membar_RSVP_' + i).val(data.checkList[selector]["RSVP"]);
                    $('#membar_E_invites_' + i).val(data.checkList[selector]["E_invites"]);
                    $('#membar_book_music_' + i).val(data.checkList[selector]["book_music"]);
                    $('#membar_licences_' + i).val(data.checkList[selector]["licences"]);
                    $('#membar_F_and_B_finalization_' + i).val(data.checkList[selector]["F_and_B_finalization"]);
                    $('#membar_choreographer_' + i).val(data.checkList[selector]["choreographer"]);
                    $('#membar_pre_wedding_shoot_' + i).val(data.checkList[selector]["pre_wedding_shoot"]);
                    $('#membar_honeymoon_booking_' + i).val(data.checkList[selector]["honeymoon_booking"]);
                    $('#membar_payment_mode_' + i).val(data.checkList[selector]["payment_mode"]);
                    $('#membar_goodie_bags_' + i).val(data.checkList[selector]["goodie_bags"]);
                    $('#membar_title3_' + i).val(data.checkList[selector]["title3"]);
                    $('#membar_thank_you_goods_' + i).val(data.checkList[selector]["thank_you_goods"]);
                    $('#membar_room_hampers_' + i).val(data.checkList[selector]["room_hampers"]);
                    $('#membar_souviners_' + i).val(data.checkList[selector]["souviners"]);
                    $('#membar_whether_prection_prevention_' + i).val(data.checkList[selector]["whether_prection_prevention"]);

                  }
                }
                $(".select2").select2();
                $(".overlay").hide();
                $(".form_add_entry").show();
              });

            });
            // alert( "Handler for .click() called." );
          });
          
          $("#erp_form").validate({
            rules: {

              guest_Name: { required: true },
              wedding_type: { required: true },
              g_Mobile: { required: true },
              g_Mail: { required: true },

            },
            errorPlacement: function (error, element) {
            }, submitHandler: function (form) {
              /* get the elements required for the post method*/
              $(".overlay").show();
              var $form = $(form);
              var url = $form.attr('action');
              if (action_type == "PUT") {
                url += SubmitUrlId;
              }
              var formData = $form.serializeObject();

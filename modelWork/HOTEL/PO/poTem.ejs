              <td style="padding:2px;border-right:1px solid#aaa;"><%=list[i].ITM_NM%></td>
              <td style="padding:2px;border-right:1px solid#aaa;text-align: right;"><%=list[i].ITM_UNIT%></td>
              <td style="padding:2px;border-right:1px solid#aaa;text-align: right;"><%=list[i].ITM_QTY.toFixed(2)%></td>
              <td style="padding:2px;border-right:1px solid#aaa;text-align: right;"><%=list[i].ITM_RATE.toFixed(2)%></td>
              <td style="padding:2px;text-align: right;"><%=list[i].ITM_AMOUNT.toFixed(2)%>/-</td>

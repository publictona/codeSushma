                    var optionList1 = '<option value=""></option>';

                    for (var i = 0; i < result.length; i++) {
                        // Concatenate each option to the optionList1 string
                        optionList1 += '<option value="' + result[i] + '">' +
                            result[i] + '</option>';
                    }
                    $('.itemCode').html(optionList1);
                    $('.itemCode').select2;
                });
